{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "intervention/image": "dev-master",
        "dingo/api": "dev-master",
        "jenssegers/mongodb": "^2.2",
        "ignited/laravel-omnipay": "2.*",
        "lokielse/omnipay-alipay": "^1.4",
        "phpding/omnipay-unionpay": "dev-master",
        "lokielse/omnipay-wechatpay": "^1.0",
        "barryvdh/laravel-cors": "^0.8.2",
        "simplesoftwareio/simple-qrcode": "~1",
        "gregwar/captcha": "1.*",
        "kbwebs/multiauth": "~1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "itsgoingd/clockwork": "^1.9"

    },
    "autoload": {
        "classmap": [
          "app/Acme",
          "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Acme\\":"app/Acme"
        },
        "files": [
            "app/helpers.php",
            "app/Libs/yunpian/YunpianAutoload.php",
            "app/Http/Controllers/Weixin/jssdk.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
